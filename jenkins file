pipeline {
     agent any
     
    tools {
        // Install the Maven version configured as "M3" and add it to the path.
         jdk 'java8'
         maven 'maven 3.3.9'
        }

    stages {
        stage('git clone') {
            steps {
                // Get some code from a GitHub repository
                git 'https://github.com/wakaleo/game-of-life.git'
                }
            }
        stage('maven build') {
            steps {
                 sh  'mvn clean install'
                }
            }
        }
    }
